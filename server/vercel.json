{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/api/plans",
      "dest": "/routes/plans.js"
    },
    {
      "src": "/api/plans(.*)",
      "dest": "/controllers/plans.js"
    }
  ]
}