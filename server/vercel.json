{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/plans",
            "dest": "/controllers/plans.js"
        },
        {
            "src": "/api/*",
            "dest": "/controllers/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
