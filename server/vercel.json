{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js"
    },
    {
      "src": "/api",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/routes/plans.js"
    },
    {
      "src": "/api/",
      "dest": "/controllers/plans.js"
    }
  ]
}