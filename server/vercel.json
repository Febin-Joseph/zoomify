{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/signin",
      "dest": "/server/controllers/auth.js"
    },
    {
      "src": "/auth/signup",
      "dest": "/server/controllers/auth.js"
    },
    {
      "src": "/auth/verifyOtp",
      "dest": "/server/controllers/auth.js"
    },
    {
      "src": "/api/plans",
      "dest": "/server/controllers/plans.js"
    },
    {
      "src": "/profile/upload/:userId",
      "dest": "/server/controllers/profile.js"
    },
    {
      "src": "/profile/:userId/image",
      "dest": "/server/controllers/profile.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ]
}