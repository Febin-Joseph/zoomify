{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/auth",
        "dest": "/routes/auth.js"
      },
      {
        "src": "/api/plans",
        "dest": "/routes/plans.js"
      },
      {
        "src": "/profile",
        "dest": "/routes/profile.js"
      },
      {
        "src": "/create/stripe/order",
        "dest": "/index.js"
      },
      {
        "src": "/create/paypal/order",
        "dest": "/index.js"
      },
      {
        "src": "/paypal/success",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }  