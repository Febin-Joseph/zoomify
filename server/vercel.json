{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/controllers/auth.js"
    },
    {
      "src": "/api/plans/(.*)",
      "dest": "/controllers/plans.js"
    },
    {
      "src": "/profile/(.*)",
      "dest": "/controllers/profile.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}