{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server"
      },
      {
        "src": "/api/plans",
        "dest": "/server/routes"
      },
      {
        "src": "/api/plans/(.*)",
        "dest": "/server/controllers/plans.js"
      }
    ]
  }  